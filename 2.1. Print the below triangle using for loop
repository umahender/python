# You are using Python
def patern(n):
    for i in range(n,0,-1):
        print((str(i)+" ")*((n-i)+1))


n=int(input())
patern(n)


#without function

def pattern(n):
    for i in range(n,0,-1):
        for j in range(i-1,n):
            print(i,end=" ")
        print()
        
n=int(input())
pattern(n)
